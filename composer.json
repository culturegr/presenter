{
  "name": "culturegr/presenter",
  "description": "Eloquent Model Presenter for Laravel applications",
  "license": "MIT",
  "authors": [
    {
      "name": "Iraklis Georgas",
      "email": "igeorgas@cutlure.gr",
      "homepage": "https://culture.gr"
    }
  ],
  "homepage": "https://github.com/culturegr/presenter",
  "keywords": [
    "Laravel",
    "Presenter"
  ],
  "require": {
    "illuminate/support": "^9.0|^10.0|^11.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.6.1",
    "orchestra/testbench": "^8.25",
    "laravel/legacy-factories": "^v1.4"
  },
  "autoload": {
    "psr-4": {
      "CultureGr\\Presenter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CultureGr\\Presenter\\Tests\\": "tests/",
      "CultureGr\\Presenter\\Tests\\Fixtures\\": "tests/fixtures/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CultureGr\\Presenter\\PresenterServiceProvider"
      ]
    }
  }
}
